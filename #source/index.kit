<!DOCTYPE html>
<html lang="ru">

<head>
	<title>Главная</title>
	<meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
	<meta name="format-detection" content="telephone=no">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">

	<link rel="stylesheet" href="css/app.css">
	<link rel="stylesheet" href="css/swiper.min.css">

	<link rel="stylesheet" href="css/style.css">

	<link rel="shortcut icon" href="favicon.ico">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
</head>

<body>
	<!-- @import "html/profile.html" -->
	<!-- @import "html/reserv.html" -->
	<!-- @import "html/cabinets.html" -->
	<!-- @import "html/register.html" -->
	<!-- @import "html/login.html" -->



	<!-- Modal -->
	<div class="modal fade" id="modal-password" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
		aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLabel">Изминение пароля</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body">
					<form action="#" class="modal-password__form">
						<label class="profile-label">
							<input type="password" class="profile-label__input input" placeholder="Введите старый пароль"
								required />
							<span class="profile-label__icon"><i class="far fa-check-circle"></i></span>
							<span class="profile-label__error"></span>
						</label>
						<label class="profile-label">
							<input type="password" class="profile-label__input input" placeholder="Введите новый пароль"
								required />
							<span class="profile-label__error"></span>
						</label>
						<label class="profile-label">
							<input type="password" class="profile-label__input input" placeholder="Повторите старый пароль"
								required />

							<span class="profile-label__icon"><i class="fas fa-minus-circle"></i></span>
							<span class="profile-label__error"></span>
						</label>
						<button type="button" class="btn btn-acent modal-password__btn">Сохранить пароль</button>
					</form>

					<a href="#" class="">Забыли пароль</a>
				</div>
			</div>
		</div>
	</div>
	<div class="modal fade" id="modal-reserv" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
		aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLabel">Вы точно хотите отменить бронирование?</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body">
					<a href="#" class="btn btn-fill">Оставить бронирование</a>
					<a href="#" class="btn btn-stroke">Отменить бронирование</a>
				</div>
			</div>
		</div>
	</div>
	<!-- @import "html/iu/js.html" -->

</body>

</html>